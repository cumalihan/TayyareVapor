#set("content") {
    <h1>City</h1>

    #if(cities) {
      <table>
        <thead>
          <tr><th>City Name</th><th>City Country</th></tr>
        </thead>
        <tbody>
          #for(city in cities) {
            <tr><td>#(city.cityName)</td><td>#(city.cityCountry)</td></tr>
          }
        </tbody>
      </table>

    } else {
      <h2> There aren't any cities yet!</h2>
    }
}

#embed("base")
